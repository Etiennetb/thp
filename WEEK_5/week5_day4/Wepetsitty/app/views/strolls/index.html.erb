<p id="notice"><%= notice %></p>

<h1>Strolls</h1>

<table>
  <thead>
    <tr>
      <th>Date</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @strolls.each do |stroll| %>
      <tr>
        <td><%= stroll.date %></td>
        <td><%= link_to 'Show', stroll %></td>
        <td><%= link_to 'Edit', edit_stroll_path(stroll) %></td>
        <td><%= link_to 'Destroy', stroll, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Stroll', new_stroll_path %>
