<p id="notice"><%= notice %></p>

<h1>Dogsitters</h1>

<table>
  <thead>
    <tr>
      <th>First name</th>
      <th>Last name</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @dogsitters.each do |dogsitter| %>
      <tr>
        <td><%= dogsitter.first_name %></td>
        <td><%= dogsitter.last_name %></td>
        <td><%= link_to 'Show', dogsitter %></td>
        <td><%= link_to 'Edit', edit_dogsitter_path(dogsitter) %></td>
        <td><%= link_to 'Destroy', dogsitter, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Dogsitter', new_dogsitter_path %>
